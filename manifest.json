{
  "name": "Meet Mute",
  "version": "1.1",
  "description": "Extensão para silenciar a aula e não precisar baixar o volume do navegador.",
  "permissions": [
    "declarativeContent",
    "activeTab",
    "storage",
    "http://meet.google.com/*",
    "https://meet.google.com/*"
  ],
  "icons": {
    "16": "images/study16.png",
    "32": "images/study32.png",
    "48": "images/study48.png",
    "128": "images/study128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/study16.png",
      "32": "images/study32.png",
      "48": "images/study48.png",
      "128": "images/study128.png"
    }
  },
  "manifest_version": 2
}